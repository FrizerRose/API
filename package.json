{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf --no-glob dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration-generate": "typeorm migration:generate -n",
    "migration-run": "typeorm migration:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "~1.5.1",
    "@nestjs/common": "~7.5.1",
    "@nestjs/config": "~0.6.3",
    "@nestjs/core": "~7.5.1",
    "@nestjs/jwt": "~7.2.0",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "~0.4.3",
    "@nestjs/swagger": "^4.7.16",
    "@nestjs/typeorm": "~7.1.5",
    "@types/dotenv": "~8.2.0",
    "aws-sdk": "~2.839.0",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "~2.0.0",
    "class-transformer": "0.3.2",
    "class-validator": "~0.13.1",
    "connect-redis": "~5.0.0",
    "crypto": "~1.0.1",
    "dotenv": "~8.2.0",
    "express-rate-limit": "^5.2.6",
    "express-session": "^1.17.2",
    "handlebars": "~4.7.7",
    "helmet": "~4.4.1",
    "ics": "~2.27.0",
    "multer": "^1.4.3",
    "multer-s3": "~2.9.0",
    "nodemailer": "^6.4.18",
    "passport": "~0.4.1",
    "passport-jwt": "~4.0.0",
    "pg": "~8.5.1",
    "redis": "~3.0.2",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "swagger-ui-express": "~4.1.6",
    "typeorm": "^0.2.37",
    "winston": "~3.3.3",
    "winston-transport-sentry-node": "~0.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.7",
    "@nestjs/schematics": "~7.1.3",
    "@nestjs/testing": "~7.5.1",
    "@types/connect-redis": "0.0.16",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.10",
    "@types/node": "^14.14.45",
    "@types/passport-jwt": "^3.0.6",
    "@types/redis": "^2.8.31",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "~4.6.1",
    "@typescript-eslint/parser": "~4.6.1",
    "eslint": "~7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "~3.1.4",
    "jest": "~26.6.3",
    "lint-staged": "~10.5.4",
    "prettier": "~2.1.2",
    "run-script-webpack-plugin": "0.0.5",
    "start-server-webpack-plugin": "~2.2.5",
    "supertest": "~6.0.0",
    "ts-jest": "~26.4.3",
    "ts-loader": "^8.0.18",
    "ts-node": "~9.0.0",
    "tsconfig-paths": "~3.9.0",
    "typescript": "~4.0.5",
    "webpack-node-externals": "~2.5.2",
    "webpack-pnp-externals": "~1.1.0",
    "yorkie": "~2.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "~.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  }
}
